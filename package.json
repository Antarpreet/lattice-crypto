{
  "name": "@atb/lattice-crypto",
  "version": "0.0.2",
  "description": "A Lattice based cryptography library for JS - Under Development",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "Lattice",
    "Crypto"
  ],
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Antarpreet Singh",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Antarpreet/lattice-crypto.git"
  },
  "bugs": {
    "url": "https://github.com/Antarpreet/lattice-crypto/issues"
  },
  "homepage": "https://github.com/Antarpreet/lattice-crypto#readme"
}
